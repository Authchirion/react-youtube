{
  "presets": [
    ["env", {
      "modules": false,
      "loose": true
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties"
  ],

  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "rollup": {
      "plugins": [
        "external-helpers"
      ]
    }
  }
}
